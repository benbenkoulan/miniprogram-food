{
    "env": {
        "browser": true,
        // "commonjs": true,
        "es6": true
    },
    "extends": [
        // "plugin:react/recommended"
        // "airbnb-base"
    ],
    // "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        // "react",
        "react-hooks"
    ],
    "rules": {
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn"
    }
    
}